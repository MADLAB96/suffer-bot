{
  "name": "suffer-bot",
  "version": "2.0.1",
  "description": "simple bot",
  "main": "src/bot.ts",
  "scripts": {
    "start": "node build/bot.js",
    "dev": "nodemon --config \"./util/nodemon.json\"/",
    "build": "rm -rf ./build/ && tsc && npm run post-build",
    "post-build": "cp ./auth.json ./build/ && cp -r ./src/data/ ./build/data",
    "test": "echo \"Error: no test specified\" && exit 1",
    "reload": "sudo cp myapp.service /etc/systemd/system/ && systemctl daemon-reload"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^12.7.12",
    "axios": "^0.19.0",
    "directory-loader": "^1.0.4",
    "discord.js": "^11.5.1",
    "discord.js-commando": "^0.10.0",
    "html2plaintext": "^2.1.2",
    "path": "^0.12.7",
    "pg": "^7.17.1",
    "sqlite3": "^4.1.1",
    "tmi.js": "^1.5.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
